{
  "name": "@loki/file-browser",
  "version": "0.2.0",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "type": "module",
  "author": "Your Name",
  "license": "MIT",
  "exports": {
    ".": "./dist/services/index.js",
    "./plugin": {
      "browser": "./dist/plugin/plugin.client.js",
      "node": "./dist/plugin/plugin.server.js",
      "default": "./dist/plugin/plugin.server.js"
    },
    "./ui/*": "./dist/ui/*",
    "./util": "./dist/shared/index.js"
  },
  "scripts": {
    "build:ui": "esbuild src/ui/*.js --bundle --format=esm --platform=browser --external:lit --outdir=dist/ui --log-level=info",
    "build:services": "esbuild src/services/index.js --bundle --format=esm --platform=node --outdir=dist/services --log-level=info",
    "build:shared": "esbuild src/shared/index.js --bundle --format=esm --platform=node --outdir=dist/shared --log-level=info",
    "build:plugin": "cp -R src/plugin dist/plugin",
    "dev:ui": "esbuild src/ui/*.js --bundle --format=esm --platform=browser --external:lit --outdir=dist/ui --log-level=info --watch",
    "dev:services": "esbuild src/services/index.js --bundle --format=esm --platform=node --outdir=dist/services --log-level=info --watch",
    "dev:shared": "esbuild src/shared/index.js --bundle --format=esm --platform=node --outdir=dist/shared --log-level=info --watch",
    "build": "npm i && rm -rf dist && npm run build:ui && npm run build:services && npm run build:shared && npm run build:plugin",
    "build:dev": "npm run dev:ui && npm run dev:services && npm run dev:shared",
    "publish:patch": "npm version patch --no-git-tag-version && npm publish",
    "examples": "npm run build && npm i --prefix examples && npm run build --prefix examples && npm start --prefix examples"
  },
  "dependencies": {
    "@loki/minihttp": "../minihttp",
    "@loki/layout": "../layout",
    "@loki/utilities": "../utilities",
    "@loki/ai-chat": "../ai-chat",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.9"
  }
}
